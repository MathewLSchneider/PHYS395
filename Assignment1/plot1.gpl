#!/usr/bin/env gnuplot

set terminal pdf enhanced
set output 'n_terms.pdf'

set xlabel 'x'
set ylabel 'y'

set style line 1 lt rgb "red"
set style line 2 lt rgb "blue"
set style line 3 lt rgb "black"


plot 'DATA2' i 0 u 1:2 w lines ls 3 title 'Actual'
replot 'DATA1' i 1 u 1:2 w lines ls 1 title '10 terms'
replot 'DATA2' i 1 u 1:2 w lines ls 2 title '100 terms'

