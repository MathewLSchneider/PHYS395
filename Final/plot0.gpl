#!/usr/bin/env gnuplot

set terminal pdf enhanced
set output 'Q3 Plots.pdf'

set xlabel 'x'
set ylabel 'y'

set style line 1 lt rgb "red"
set style line 2 lt rgb "blue"
set style line 3 lt rgb "black"


plot 'DATA' u 1:2 w l ls 1 title 'Position'
plot 'DATA' u 1:3 w l ls 1 title 'Velocity'
plot 'DATA' u 1:4 w l ls 1 title 'Energy Conservation'



